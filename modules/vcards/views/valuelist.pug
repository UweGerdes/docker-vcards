if fields[field].parts
  span.parts
    each part, i in fields[field].parts
      if i > 0 && value[fields[field].parts[i]]
        = ', '
      span(title=part)
        = value[part]
else
  ul.values
    each obj in value
      li.values-item
        = obj.value
        if obj.type
          = ' (' + type(obj.type).join(', ') + ')'
